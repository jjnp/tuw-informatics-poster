
{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"importHelpers": true,
		"strict": false,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true,
		"paths": {
			"$components/*": ["src/componens/*"],
			"$assets/*": ["static/*"],
			"$styles/*": ["src/styles/*"]
		},
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.ts", "src/**/*.svelte", "test/**/*.ts"]
}
